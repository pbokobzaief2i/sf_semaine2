{% extends 'base.html.twig' %}

{% block title %}La liste des livres{% endblock %}

{% block body %}
<div class="example-wrapper">
    <h2>Liste des livres</h1>
    <ul>
        {%for book in books %}
        <li>
            <a href="/book/{{book.title}}-{{book.id}}">{{ book.title }}</a>
        </li>
        {% endfor %}
    </ul>
</div>
{% endblock %}
